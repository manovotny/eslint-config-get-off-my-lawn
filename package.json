{
    "name": "eslint-config-get-off-my-lawn",
    "description": "A highly opinionated, sharable config of ESLint rules to produce beautiful, readable JavaScript.",
    "version": "4.0.1",
    "author": {
        "name": "Michael Novotny",
        "email": "manovotny@gmail.com",
        "url": "https://manovotny.com"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/manovotny/eslint-config-get-off-my-lawn.git"
    },
    "keywords": [
        "babel",
        "config",
        "eslint",
        "react"
    ],
    "engines": {
        "node": ">=8.6.0",
        "yarn": ">=1.0.0"
    },
    "main": "index.js",
    "scripts": {
        "prettier": "prettier --write \"**/*.{js,json,md,yml}\"",
        "test": "eslint . --ext .js --ext .json"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,json,md,yml}": [
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {
        "babel-eslint": "8.2.2",
        "eslint-plugin-eslint-comments": "2.0.2",
        "eslint-plugin-get-off-my-lawn": "1.0.2",
        "eslint-plugin-import": "2.10.0",
        "eslint-plugin-jest": "21.15.0",
        "eslint-plugin-json": "1.2.0",
        "eslint-plugin-jsx-a11y": "6.0.3",
        "eslint-plugin-node": "6.0.1",
        "eslint-plugin-objects": "1.1.1",
        "eslint-plugin-react": "7.7.0",
        "eslint-plugin-security": "1.4.0",
        "eslint-plugin-unicorn": "4.0.2"
    },
    "devDependencies": {
        "eslint": "5.14.1",
        "eslint-config-prettier": "2.9.0",
        "husky": "1.3.1",
        "lint-staged": "8.1.4",
        "prettier": "1.16.4"
    },
    "peerDependencies": {
        "eslint": ">=5.14.1"
    }
}
